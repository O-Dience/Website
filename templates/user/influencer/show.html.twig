{% extends 'base.html.twig' %}

{% block title %}
    {{ parent() }} - {{ influencer.username }}
{% endblock %}

{% block body %}

<div class="d-flex justify-content-between align-items-start mt-5">
    <h1>{{ influencer.username }}</h1>
</div>


{% if influencer.picture is not empty %}
<img src="{{ asset('uploads/avatars/' ~ influencer.picture) }}">
{% endif %}

{% endblock %}